<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="contact_view_extend_climbing_school" model="ir.ui.view">
        <field name="name">res.partner.form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='climbing_gym_data']" position="inside">
                <group string="Courses" name="course_student" priority="2" col="4">
                    <field name='climbing_gym_school_course_student' nolabel="1"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
